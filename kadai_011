# Forの利用の記述
list = ["水", "金", "地", "火", "木", "土", "天","海", "冥"]
for planet in list:
    print(planet)

# whileの利用の記述
list = ["水", "金", "地", "火", "木", "土", "天","海", "冥"]
while(True):
    print(list[0])
    print(list[1])
    print(list[2])
    print(list[3])
    print(list[4])
    print(list[5])
    print(list[6])
    print(list[7])
    print(list[8])
    break
